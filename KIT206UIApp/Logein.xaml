<Page x:Class="KIT206.DatabaseApp.UI.Logein"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:KIT206.DatabaseApp.UI"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Logein">

    <Grid>
		<StackPanel Orientation="Horizontal" Height="35" VerticalAlignment="Top">
			<Button Content="goPage1" MinWidth="100" Margin="10,0,0,0" Click="BtnClick1" />
			<Button Content="goPage2" MinWidth="100" Margin="10,0,0,0" Click="BtnClick2" />
		</StackPanel>
		<Frame x:Name="Main" Margin="0,35,0,0"/>
    </Grid>
</Page>
